{
  "name": "@notifee/react-native",
  "version": "0.5.1",
  "author": "Invertase <oss@invertase.io> (http://invertase.io)",
  "description": "Notifee - a feature rich notifications library for React Native.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "genversion --es6 --semi src/version.ts && tsc",
    "build:core": "cd ../../android && ./gradlew assembleRelease",
    "build:clean": "rimraf android/libs/notifee_core_debug.aar && rimraf android/build && rimraf ios/build && rimraf dist",
    "build:watch": "tsc --watch",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run build:core",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@types/jest": "^25.2.1",
    "@types/react-native": "^0.60.31",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "genversion": "^2.2.0",
    "jest": "^25.5.1",
    "prettier": "^1.19.1",
    "react-native": "^0.62.2",
    "rimraf": "^2.7.1",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react-native": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/notifee/notifee"
  },
  "license": "Apache-2.0",
  "keywords": [
    "react",
    "react-native",
    "scheduling",
    "channels",
    "android",
    "ios",
    "firebase",
    "fcm",
    "notifee",
    "notifications",
    "onesignal"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/preset-flow": "^7.9.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react": "^16.13.1"
  }
}
